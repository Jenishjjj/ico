<%- include('header') %>
    <div class="page-content">
        <div class="container">
            <div class="content-area card">
                <div class="card-innr">
                    <div class="card-head">
                        <h4 class="card-title">KYC List</h4>
                    </div>
                    <table class="data-table dt-init kyc-list kyclist">
                        <thead>
                            <tr class="data-item data-head">
                                <th class="data-col dt-user">User</th>
                                <th class="data-col dt-doc-type">Nationality</th>
                                <th class="data-col dt-doc-front">KYC Documnets</th>
                                <th class="data-col dt-status">Status</th>
                                <th class="data-col"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <%for(var i of data){%>
                                <tr class="data-item">
                                    <td class="data-col dt-user">
                                        <span class="lead user-name">
                                            <%=i.First_Name%>
                                        </span>
                                        <span class="sub user-id">
                                            <%=i.Email%>
                                        </span>
                                    </td>
                                    <td class="data-col dt-doc-type">
                                        <span class="sub sub-s2 sub-dtype">
                                            <%=i.Nationality%>
                                        </span>
                                    </td>
                                    <td class="data-col dt-doc-front">

                                        <% if (i.KYC_Doc && i.KYC_Doc !=='Not Submitted' ) { %>


                                        <a href="<%= i.KYC_Doc %>" class="image-popup">View Document</a> 


                                            <% } else if (i.KYC_Doc==='Not Submitted' ) { %>
                                                <!-- <p>KYC Document not available.</p> -->
                                                <% } else { %>
                                                    <p>NULL</p>
                                                    <% } %>
                                                    <% if (i.KYC_Front_Image && i.KYC_Front_Image !=='Not Submitted' ) { %>
                                        <a href="<%= i.KYC_Front_Image %>" class="image-popup">Front Document</a> 

                                                        <% } else if (i.KYC_Front_Image==='Not Submitted' ) { %>
                                                            <!-- <p>KYC Document not available.</p> -->
                                                            <% } else { %>
                                                                <p>NULL</p>
                                                                <% } %>
            
                                                                    <% if (i.KYC_Back_Image && i.KYC_Back_Image !=='Not Submitted' )
                                                                        { %>
                                                                        
                                                                          ||  <a href="<%= i.KYC_Back_Image %>" class="image-popup">Back Document</a> 

                                                                        <% } else if (i.KYC_Back_Image ==='Not Submitted' ) { %>
                                                                            <!-- <p>KYC Document not available.</p> -->
                                                                            <% } else { %>
                                                                                <p>NULL</p>
                                                                                <% } %>
                                    </td>
                              
                                    <td class="data-col dt-status">
                                        <% if (i.Status ==='Pending' ) { %>
                                            <span class="dt-status-md badge btn btn-warning badge-md">Pending</span>

                                            <% } else if (i.Status ==='Approved' ) { %>
                                                <span class="dt-status-md btn btn-success badge-md">Approved</span>

                                                <% } else if (i.Status ==='Cancelled' ) { %>
                                                    <span class="dt-status-md badge btn btn-danger badge-md">Rejected</span>

                                                    <% } %>
                                    </td>
                                    <td class="data-col text-right">
                                        <div class="relative d-inline-block">
                                            <a href="#" class="btn btn-light-alt btn-xs btn-icon toggle-tigger"><em
                                                    class="ti ti-more-alt"></em></a>
                                            <div class="toggle-class dropdown-content dropdown-content-top-left">
                                                <ul class="dropdown-list">
                                                    <li> <a href="/kyc/kycListDetail/<%= i.id %>"><em
                                                                class="ti ti-eye"></em> View
                                                            Details</a></li>
                                                    <li><a href="/kyc/Approve/<%= i.id %>"><em class="ti ti-check"></em> Approve</a></li>
                                                    <li><a href="/kyc/Cancelled/<%= i.id %>"><em class="ti ti-na"></em> Reject</a></li>
                                                    <li><a href="#"><em class="ti ti-trash"></em> Delete</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </td>
                                </tr><!-- .data-item -->
                                <%}%>
                                
                        </tbody>
                    </table>
                </div><!-- .card-innr -->
            </div><!-- .card -->
        </div><!-- .container -->
    </div>
    <%- include('footer') %>